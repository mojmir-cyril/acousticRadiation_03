<extension version="1" name="acousticRadiation">
    <script src="main.py"/>
    <script src="ObjectController.py"/>
    <interface context="Mechanical">
        <images>images</images>
        <callbacks>
            <oninit>onInit</oninit>
            <onload>onLoad</onload>
            <onReady>OnReady</onReady>
            <resume>Resume</resume>

        </callbacks>
        <toolbar name="acousticRadiation" caption="acousticRadiation">
            <entry name="Evaluated at Single Frequency" icon="icon_environment_acoustic_radiation_boundary_16x16">
                <entry name="ERP" icon="icon_environment_acoustic_radiation_boundary_16x16">
                    <callbacks>
                        <onclick>CreateERPObj</onclick>
                    </callbacks>
                </entry>
                <entry name="ERP Level" icon="icon_environment_acoustic_radiation_boundary_16x16">
                    <callbacks>
                        <onclick>CreateERPLevelObj</onclick>
                    </callbacks>
                </entry>
                <entry name="Normal Velocity" icon="icon_environment_acoustic_radiation_boundary_16x16">
                    <callbacks>
                        <onclick>CreateNormalVelObj</onclick>
                    </callbacks>
                </entry>
            </entry>
             <entry name="Evaluated over Frequency Range" icon="icon_environment_acoustic_radiation_boundary_16x16">
                <entry name="ERP" icon="icon_environment_acoustic_radiation_boundary_16x16">
                    <callbacks>
                        <onclick>CreateERPRangeObj</onclick>
                    </callbacks>
                </entry>
                <entry name="ERP Level" icon="icon_environment_acoustic_radiation_boundary_16x16">
                    <callbacks>
                        <onclick>CreateERPLevelRangeObj</onclick>
                    </callbacks>
                </entry>
                <entry name="Normal Velocity" icon="icon_environment_acoustic_radiation_boundary_16x16">
                    <callbacks>
                        <onclick>CreateNormalVelRangeObj</onclick>
                    </callbacks>
                </entry>
            </entry>
        </toolbar>
    </interface>

    <simdata context="Mechanical">
        <object name="ERPPostObj" version="1" caption="ERP" class="ObjectController"
                icon="icon_environment_acoustic_radiation_boundary_16x16" color="transparent">
            <target type="analysis" location="post"/>

            <propertygroup name="Settings">
                <property name="NonVisibleGeometry" caption="Geometry" control="scoping" visible="false"></property>
                <property name="Geometry" caption="Geometry" control="scoping"></property>
                <property name="Frequency" caption="Frequency [Hz]" control="float">
                    <callbacks><onvalidate>onValidate</onvalidate></callbacks>
                    <callbacks><isvalid>isValidFreq</isvalid></callbacks>
                </property>
                <property name="numberOfColors" caption="Number of color bands" control="integer" default="9"></property>
            </propertygroup>
            <propertygroup name="Results">
                <property name="OverallERP" caption="Overall ERP" control="float" readonly="true" unit="heat rate" default="0 [W]" isparameter="true"></property>
                <property name="OverallERPlevel" caption="Overall ERP level" control="float" readonly="true" unit="sound pressure level" default="0 [dB]" isparameter="true"></property>
                <property name="dataDict" caption="dataDict" control="float" readonly="true" visible="false"></property>
                <property name="specDataDict" caption="specDataDict" control="float" readonly="true" visible="false"></property>
                <property name="dictResults" caption="dictResults" control="float" readonly="true" visible="false"></property>

            </propertygroup>
        </object>

        <object name="ERPLevelPostObj" version="1" caption="ERP Level" class="ObjectController"
                icon="icon_environment_acoustic_radiation_boundary_16x16" color="transparent">
            <target type="analysis" location="post"/>

            <propertygroup name="Settings">
                <property name="NonVisibleGeometry" caption="Geometry" control="scoping" visible="false"></property>
                <property name="Geometry" caption="Geometry" control="scoping"></property>
                <property name="Frequency" caption="Frequency [Hz]" control="float">
                    <callbacks><onvalidate>onValidate</onvalidate></callbacks>
                    <callbacks><isvalid>isValidFreq</isvalid></callbacks>
                </property>
                <property name="numberOfColors" caption="Number of color bands" control="integer" default="9"></property>
            </propertygroup>
            <propertygroup name="Results">
                <property name="OverallERP" caption="Overall ERP" control="float" readonly="true" unit="heat rate" default="0 [W]" isparameter="true"></property>
                <property name="OverallERPlevel" caption="Overall ERP level" control="float" readonly="true" unit="sound pressure level" default="0 [dB]" isparameter="true"></property>
                <property name="dataDict" caption="dataDict" control="float" readonly="true" visible="false"></property>
                <property name="specDataDict" caption="specDataDict" control="float" readonly="true" visible="false"></property>
                <property name="dictResults" caption="dictResults" control="float" readonly="true" visible="false"></property>
            </propertygroup>
        </object>

        <object name="NormalVelPostObj" version="1" caption="Normal Velocity" class="ObjectController"
                icon="icon_environment_acoustic_radiation_boundary_16x16" color="transparent">
            <target type="analysis" location="post"/>

            <propertygroup name="Settings">
                <property name="NonVisibleGeometry" caption="Geometry" control="scoping" visible="false"></property>
                <property name="Geometry" caption="Geometry" control="scoping"></property>
                <property name="Frequency" caption="Frequency [Hz]" control="float">
                    <callbacks><onvalidate>onValidate</onvalidate></callbacks>
                    <callbacks><isvalid>isValidFreq</isvalid></callbacks>
                </property>
                <property name="numberOfColors" caption="Number of color bands" control="integer" default="9"></property>

            </propertygroup>
            <propertygroup name="Results">
                <property name="MinVelocity" caption="Min Velocity" control="float" readonly="true" unit="velocity" default="0 [m s^-1]" isparameter="true"></property>
                <property name="MaxVelocity" caption="Max Velocity" control="float" readonly="true" unit="velocity" default="0 [m s^-1]" isparameter="true"></property>
                <property name="dataDict" caption="dataDict" control="float" readonly="true" visible="false"></property>
                <property name="specDataDict" caption="specDataDict" control="float" readonly="true" visible="false"></property>
                <property name="dictResults" caption="dictResults" control="float" readonly="true" visible="false"></property>
            </propertygroup>
        </object>



        <object name="ERPRangePostObj" version="1" caption="ERP"
                icon="icon_environment_acoustic_radiation_boundary_16x16" color="transparent">
            <target type="analysis" location="post"/>

            <propertygroup name="Settings">
                <property name="NonVisibleGeometry" caption="Geometry" control="scoping" visible="false"></property>
                <property name="Geometry" caption="Geometry" control="scoping"></property>
                <property name="FrequencyRange" caption="Frequency Range" control="select" default="Full Range">
                    <attributes options="Full Range, User Defined"/>
                </property>
                <property name="MinFrequency" caption="Min Frequency [Hz]" control="float" >
                    <callbacks><onvalidate>onValidate</onvalidate></callbacks>
                    <callbacks><isvalid>isValidFreq</isvalid></callbacks>
                </property>
                <property name="MaxFrequency" caption="Max Frequency [Hz]" control="float">
                    <callbacks><onvalidate>onValidate</onvalidate></callbacks>
                    <callbacks><isvalid>isValidFreq</isvalid></callbacks>
                </property>
                <property name="numberOfColors" caption="Number of color bands" control="integer" default="9"></property>
            </propertygroup>
            <propertygroup name="Results">
                <property name="OverallERP" caption="Overall ERP" control="float" readonly="true" unit="heat rate" default="0 [W]" isparameter="true"></property>
                <property name="OverallERPlevel" caption="Overall ERP level" control="float" readonly="true" unit="sound pressure level" default="0 [dB]" isparameter="true"></property>
            </propertygroup>
        </object>

        <object name="ERPLevelRangePostObj" version="1" caption="ERP Level"
                icon="icon_environment_acoustic_radiation_boundary_16x16" color="transparent">
            <target type="analysis" location="post"/>

            <propertygroup name="Settings">
                <property name="NonVisibleGeometry" caption="Geometry" control="scoping" visible="false"></property>
                <property name="Geometry" caption="Geometry" control="scoping"></property>
                <property name="Frequency" caption="Frequency [Hz]" control="float">
                    <callbacks><onvalidate>onValidate</onvalidate></callbacks>
                    <callbacks><isvalid>isValidFreq</isvalid></callbacks>
                </property>
                <property name="numberOfColors" caption="Number of color bands" control="integer" default="9"></property>
            </propertygroup>
            <propertygroup name="Results">
                <property name="OverallERP" caption="Overall ERP" control="float" readonly="true" unit="heat rate" default="0 [W]" isparameter="true"></property>
                <property name="OverallERPlevel" caption="Overall ERP level" control="float" readonly="true" unit="sound pressure level" default="0 [dB]" isparameter="true"></property>
            </propertygroup>
        </object>

        <object name="NormalVelRangePostObj" version="1" caption="Normal Velocity"
                icon="icon_environment_acoustic_radiation_boundary_16x16" color="transparent">
            <target type="analysis" location="post"/>

            <propertygroup name="Settings">
                <property name="NonVisibleGeometry" caption="Geometry" control="scoping" visible="false"></property>
                <property name="Geometry" caption="Geometry" control="scoping"></property>
                <property name="Frequency" caption="Frequency [Hz]" control="float">
                    <callbacks><onvalidate>onValidate</onvalidate></callbacks>
                    <callbacks><isvalid>isValidFreq</isvalid></callbacks>
                </property>
                <property name="numberOfColors" caption="Number of color bands" control="integer" default="9"></property>

            </propertygroup>
            <propertygroup name="Results">
                <property name="MinVelocity" caption="Min Velocity" control="float" readonly="true" unit="velocity" default="0 [m s^-1]" isparameter="true"></property>
                <property name="MaxVelocity" caption="Max Velocity" control="float" readonly="true" unit="velocity" default="0 [m s^-1]" isparameter="true"></property>
            </propertygroup>
        </object>



    </simdata>
</extension>
